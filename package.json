{
  "name": "eofol-head",
  "version": "1.0.0",
  "description": "Render HTML templates from boilerplate including helmet for head tag",
  "license": "MIT",
  "author": "Jakub Eliáš <wurducius@gmail.com>",
  "type": "commonjs",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -outDir ./dist -rootDir ./src",
    "clean": "node ./script/clean.js",
    "rebuild": "npm run clean && npm run build",
    "reinstall": "node ./script/reinstall.js",
    "reinstall-f": "node ./script/reinstall.js -f",
    "check": "npx eslint -c eslint.config.mjs .",
    "check-fix": "npx eslint -c eslint.config.mjs --fix .",
    "prettify": "npx prettier --config .prettierrc --write .",
    "lint": "npm run check-fix && npm run prettify"
  },
  "dependencies": {
    "html-to-json-parser": "^2.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/node": "^22.10.5",
    "eslint": "^9.17.0",
    "globals": "^15.14.0",
    "rimraf": "^6.0.1",
    "prettier": "3.4.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.19.1"
  },
  "homepage": "https://github.com/wurducius/eofol-head#readme",
  "bugs": {
    "url": "https://github.com/wurducius/eofol-head/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wurducius/eofol-head.git"
  }
}
